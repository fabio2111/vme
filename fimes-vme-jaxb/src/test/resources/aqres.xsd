				<Description>contains one or more standard term (e.g. "overexploited", "fully exploited") describing the state of an aquatic resource.</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:attribute name="Value" use="required">
				<xs:simpleType>
					<xs:restriction base="xs:token">
						<xs:enumeration value="Depleted"/>
						<xs:enumeration value="Fully exploited"/>
						<xs:enumeration value="Recovering"/>
						<xs:enumeration value="Moderately exploited"/>
						<xs:enumeration value="Overexploited"/>
						<xs:enumeration value="Uncertain"/>
						<xs:enumeration value="Underexploited"/>
